{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "functions": {
    "app/api/process/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/api/process",
      "methods": ["POST"],
      "headers": {
        "x-vercel-payload-max-size": "10485760"
      }
    }
  ]
}
